//Colores base

azul = #1E88E5 //provicional
azul_claro = #42A5F5 

body
	margin 0
	padding 0
	font-family Arial
	
#menu-off
	background azul
	width 100%
	& ul
		margin 0
		padding 0
		& .page-cont
			margin-left 2em
		& .sessions-cont
			float right
		& div
			display inline-block
			& a
				color white
				display inline-block
				text-decoration none
				padding .5em .8em
				& li
					list-style none
				
		
